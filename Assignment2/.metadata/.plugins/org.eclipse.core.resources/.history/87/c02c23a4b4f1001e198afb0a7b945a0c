public class InvoiceTester {

	public static void main(String[] args) 
	{
		// TODO Auto-generated method stub
		Invoice i1=new Invoice();
		System.out.println("Part no:"+i1.getPartno());
	//	System.out.println("Part no:"+i1.setPartno());
